		"@types/marked": "^5.0.2",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.4",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.0.0",
